{
  "name": "Chrome Github Trending Sidebar",
  "version": "0.1.0",
  "description": "Example chrome sidebar extension to embed an iframe.",
  "web_accessible_resources": [
    "entry.js"
  ],
  "background": {
    "scripts": [
      "background.js"
    ],
    "persistent": true
  },
  "browser_action": {
    "default_title": "Chrome Github Trending Sidebar"
  },
  "externally_connectable": {
    "matches": ["*://*.syncta.com/*"]
  },
  "content_scripts": [
    {
      "matches": ["https://*.syncta.com/*"],
      "js": ["abc.js"]
    }
  ],
  "manifest_version": 2,
  "permissions": [
    "https://*/*",
    "http://*/*",
    "tabs",
    "webRequest",
    "webRequestBlocking",
    "activeTab"
  ]
}
